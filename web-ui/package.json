{
  "name": "voice-ferry-web-ui",
  "version": "1.0.0",
  "description": "Web interface for Voice Ferry Class 4 Switch configuration and monitoring",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon --inspect=0.0.0.0:9229 server.js",
    "dev:debug": "node --inspect-brk=0.0.0.0:9229 server.js",
    "dev:watch": "nodemon --watch . --ext js,json,yml,yaml server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js --fix",
    "lint:check": "eslint . --ext .js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build": "echo 'No build step required for this application'",
    "docker:build": "docker build -t voice-ferry-ui .",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:prod": "docker-compose up",
    "clean": "rm -rf node_modules package-lock.json",
    "reset": "npm run clean && npm install",
    "logs": "docker-compose logs -f voice-ferry-ui",
    "shell": "docker-compose exec voice-ferry-ui sh"
  },
  "keywords": [
    "sip",
    "b2bua",
    "voice",
    "telecommunications",
    "webui",
    "dashboard"
  ],
  "author": "Voice Ferry Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "ws": "^8.14.2",
    "@grpc/grpc-js": "^1.9.14",
    "@grpc/proto-loader": "^0.7.10",
    "redis": "^4.6.10",
    "etcd3": "^1.1.2",
    "yaml": "^2.3.4",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "babel-loader": "^9.1.3",
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
    "html-webpack-plugin": "^5.6.0",
    "copy-webpack-plugin": "^11.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
